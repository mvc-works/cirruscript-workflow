    "del": "^1.2.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^5.0.2",
    "hsl": "^0.1.1",
    "http-server": "^0.12.0",
    "json-loader": "^0.5.3",
    "run-sequence": "^1.1.2",
